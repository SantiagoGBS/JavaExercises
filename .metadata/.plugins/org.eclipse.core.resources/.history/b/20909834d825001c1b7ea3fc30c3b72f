package CondicionalesyBucles;

import java.util.Scanner;

public class Ejercicio24 {

	public static void main(String[] args) {
		//24. Dibuja un cuadrado de n elementos de lado utilizando *.
		int n=1;
		int i=0;
		Scanner s=new Scanner(System.in);
		n=s.nextInt();
		
		while(i<=i*n) {
			System.out.print("*");
			//if(i==n) {
				System.out.println("*");
			//}
			
			i++;
		}
			
		}

}
